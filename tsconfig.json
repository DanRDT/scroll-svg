{
  "compilerOptions": {
    "module": "es6",
    "target": "es5",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "moduleResolution": "node"
  }
}
